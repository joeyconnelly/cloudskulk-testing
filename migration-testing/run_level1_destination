
# ***ONLY RUN THIS ON HOST***

# host dest
# Migrating to this paused qemu image (host source -> host dest):

export NO_AT_BRIDGE=1

qemu-system-x86_64 \
-name mig-L1-Dest \
-m size=512M,slots=1,maxmem=1024M \
-boot order=c \
-drive file=/tmp/guest-imgs/copyRAW-60G.img,media=disk,format=raw,cache=writeback,aio=threads,if=virtio \
-machine type=pc-i440fx-2.3 \
-cpu Nehalem \
-enable-kvm \
-vga qxl \
-show-cursor \
-device virtio-net-pci,netdev=netL1Dest \
-netdev user,id=netL1Dest \
-incoming tcp:0:4446 &

