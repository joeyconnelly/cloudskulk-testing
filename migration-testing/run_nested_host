
# ***ONLY RUN THIS ON HOST***

# nested level2 dest
# Migrating to this paused qemu image (host source -> level1 -> level2 dest):

export NO_AT_BRIDGE=1

qemu-system-x86_64 \
-name mig-L1-Nest \
-m size=14G \
-boot order=c \
-drive file=/tmp/guest-imgs/diskRAW-120G.img,media=disk,format=raw,cache=writeback,aio=threads,if=virtio \
-machine type=pc-i440fx-2.3 \
-cpu host,+vmx \
-enable-kvm \
-vga qxl \
-show-cursor \
-device virtio-net-pci,netdev=netL1Nest \
-netdev user,id=netL1Nest,hostfwd=tcp:0:4446-:5556 &

